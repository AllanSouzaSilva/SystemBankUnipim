@model IEnumerable<Systembankunipim.Data.TbInvestimento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Quantidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCarteiraNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoInvestimentoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Quantidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCarteiraNavigation.IdCarteira)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoInvestimentoNavigation.DescricaoInvestimento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdInvestimento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdInvestimento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdInvestimento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
